programa {
	funcao inicio() {
		inteiro valor 
		escreva ("Qual o valor: ")
		leia (valor)
		 
		se (valor > 20)
	  { 
	    escreva ("O numero é maior que 20")
	  }
	    senao se (valor==20)
	  {
	    escreva ("o numero é igual a 20 ")
	  }
	    
	    senao 
	  { 
	    escreva ("O numero é menor que 20")
	  }
	    
}
}